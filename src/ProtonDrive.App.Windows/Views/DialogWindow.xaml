<windows:CustomChromeWindow
    x:Class="ProtonDrive.App.Windows.Views.DialogWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:loc="clr-namespace:ProtonDrive.App.Windows.Resources"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:ProtonDrive.App.Windows.Views"
    xmlns:windows="clr-namespace:ProtonDrive.App.Windows.Toolkit.Windows"
    Title="{Binding Path=(views:IDialogViewModel.Title), Mode=OneWay, TargetNullValue={loc:Resource Main_Window_Title}}"
    Content="{Binding Mode=OneWay}"
    Style="{StaticResource DialogStyle}"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance views:IDialogViewModel}" />
